.navbar{
  flex: none;
  display: flex;
  background: #fff;
  border:1rpx solid #eceef3;

}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  height: 94rpx;
  line-height: 94rpx;
  font-size: 32rpx
}
.navbar .item.active{
  color: #26b13c;
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background: #26b13c;
 
}
